    "def is_leap(year):\n",
    "    \n",
    "    if(year %4) == 0:\n",
    "        return True\n",
    "    if year % 100 == 0 and year % 400:\n",
    "        return True\n",
    "    else:\n",
    "        return False\n",
    " \n",
    "   \n",
    "\n",
    "\n",
    "year = int(input())\n",
    "print(\"Is it a leap year? ,{}\".format(is_leap(year)))"
